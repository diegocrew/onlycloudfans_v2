{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public", 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "onlycloudfans.web.app",
        "destination": "https://onlycloudfans.com", 
        "type": 301
      },
      {
        "source": "onlycloudfans.firebaseapp.com",
        "destination": "https://onlycloudfans.com", 
        "type": 301
        }
      ]
    }
  }
  